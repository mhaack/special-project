{
  "groups": [
    {
      "title": "Default Content",
      "id": "default",
      "components": [
        {
          "title": "Text",
          "id": "text",
          "plugins": {
            "da": {
              "name": "text",
              "type": "text"
            }
          }
        },
        {
          "title": "Image",
          "id": "image",
          "plugins": {
            "da": {
              "name": "image",
              "type": "image"
            }
          }
        }
      ]
    },
    {
      "title": "Sections",
      "id": "sections",
      "components": [
        {
          "title": "Section",
          "id": "section",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>"
            }
          }
        }
      ]
    },
    {
      "title": "Blocks",
      "id": "blocks",
      "components": [
        {
          "title": "Cards",
          "id": "cards",
          "plugins": {
            "da": {
              "name": "cards",
              "rows": 1,
              "columns": 2
            }
          }
        },
        {
          "title": "Card",
          "id": "card",
          "plugins": {
            "da": {
              "name": "card",
              "rows": 2,
              "columns": 0
            }
          }
        },
        {
          "title": "Columns",
          "id": "columns",
          "plugins": {
            "da": {
              "template": "<div class=\"columns\"><div><div></div><div></div></div></div>"
            }
          }
        },
        {
          "title": "Fragment",
          "id": "fragment",
          "plugins": {
            "da": {
              "template": "<div class=\"fragment\"><div><div></div></div></div>"
            }
          }
        },
        {
          "title": "Hero",
          "id": "hero",
          "plugins": {
            "da": {
              "template": "<div class=\"hero\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture><h1></h1></div></div></div>"
            }
          }
        }
      ]
    }
  ]
}